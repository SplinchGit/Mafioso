# Environment Variables for Local Development
# Copy this file to .env and update with your values

# JWT Secret for token signing
JWT_SECRET=mafioso-dev-secret-change-in-production

# World ID Configuration
WORLD_ID_APP_ID=app_staging_123456789
WORLD_ID_API_KEY=your_world_id_api_key_here

# DynamoDB Table Names (auto-configured in AWS)
PLAYERS_TABLE=mafioso-players
WORLD_ID_TABLE=mafioso-worldid
COOLDOWNS_TABLE=mafioso-cooldowns
CRIME_HISTORY_TABLE=mafioso-crime-history

# AWS Configuration (optional for local development)
AWS_REGION=us-east-1